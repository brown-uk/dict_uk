# Безособова форма дієслів -ти -> -но

subgroup v.imprsNo

ти      но      ти              #  казати казано                    @ verb:impers


# Безособова форма дієслів -ти -> -то

subgroup v.imprsTo

ти	то	ти	                #  вимити вимито              @ verb:impers


